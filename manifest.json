{
  "manifest_version": 3,
  "name": "The Wacky Extension",
  "version": "1.0",
  "description": "Makes the web very wacky.",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Wackifies the web"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}